#!/bin/bash

echo "compiling project ..."
lb compile project apisan.project

echo "making database ..."
lb delete DB
lb create DB

echo "adding project ..."
lb addproject DB .

echo "adding facts ..."
lb import-protobuf DB traces Traces "$1"

echo "printing results ..."
lb query DB '_(x, z) <- Buggy(x, y), traces:CallEvent_code[y] = z.'
